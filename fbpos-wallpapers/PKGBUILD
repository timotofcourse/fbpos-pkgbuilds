package() {
    mkdir "${pkgdir}/usr"
    mkdir "${pkgdir}/usr/share"
    mkdir "${pkgdir}/usr/share/wallpapers"
    mv -f "${srcdir}/*.png" "${srcdir}/fbpos/"
    mv -f "${srcdir}/*.jpg" "${srcdir}/fbpos/"
    cp -R "${srcdir}/fbpos" "${pkgdir}/usr/share/wallpapers/"
}
